{
  "name": "my-app",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "Quasar App",
  "author": "davwys <david.wyss@hotmail.ch>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "dev:pwa": "quasar dev -m pwa",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "build:ssr": "quasar build -m ssr",
    "android": "quasar dev -m capacitor -T android",
    "ios": "quasar dev -m capacitor -T ios",
    "test:unit": "jest --updateSnapshot",
    "test:unit:report": "jest --ci --coverage",
    "serve:test:report": "quasar serve reports/coverage/lcov-report/ --port 8788",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "concurrently:dev:test": "concurrently \"yarn dev\" \"yarn test:unit:watch\""
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@aws-sdk/client-ses": "^3.41.0",
    "@capacitor/android": "^3.2.5",
    "@capacitor/cli": "^3.2.5",
    "@capacitor/core": "^3.2.5",
    "@capacitor/ios": "^3.2.5",
    "@quasar/extras": "^1.0.0",
    "@vue/apollo-composable": "^4.0.0-alpha.15",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "amazon-cognito-identity-js": "^5.2.0",
    "axios": "^0.24.0",
    "core-js": "3.19.1",
    "date-fns": "^2.25.0",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "jest-sonar-reporter": "^2.0.0",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "qrcode.vue": "^3.3.3",
    "quasar": "^2.0.0",
    "subscriptions-transport-ws": "^0.9.19",
    "vue": "^3.2.20",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.0.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^16.8.1",
    "vue-router": "4",
    "vuex": "^4.0.1",
    "vuex-smart-module": "^1.0.0"
